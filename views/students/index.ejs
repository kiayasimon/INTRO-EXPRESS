<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>First Express</title>
</head>
<body>
    <h1>Students</h1>
    <ul>
     <% students.forEach(function(s) { %>
        <li>
          <%= s.student %>  
            -
            <%= s.enrolled ? 'done' : 'not done' %>
        </li>
     <% }); %>
    </ul>
</body>
</html>